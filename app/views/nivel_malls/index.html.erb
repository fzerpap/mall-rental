<h1>Listing nivel_malls</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nivel_malls.each do |nivel_mall| %>
      <tr>
        <td><%= nivel_mall.nombre %></td>
        <td><%= link_to 'Show', nivel_mall %></td>
        <td><%= link_to 'Edit', edit_nivel_mall_path(nivel_mall) %></td>
        <td><%= link_to 'Destroy', nivel_mall, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nivel mall', new_nivel_mall_path %>
